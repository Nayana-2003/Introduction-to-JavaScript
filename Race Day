let raceNumber = Math.floor(Math.random() * 1000);
let runnerRegEarly=true;
let runnerAge=20;
if(runnerAge>18 && runnerRegEarly===true)
{
  raceNumber+=1000;
}
if(runnerAge>18 && runnerRegEarly===true)
{
  console.log(`Adults registerd early will race at 9:30 am.Your Race Number is ${raceNumber}.`);
}
else if(runnerAge>18 && runnerRegEarly===false)
{
  console.log(`Adults registerd late will race at 11 am.Your Race Number is ${raceNumber}.`);
}
else if(runnerAge<18)
{
  console.log(`Youth registrants will race at 12:30 pm.Your Race Number is ${raceNumber}.`);
}
else 
{
  console.log(`See the registration desk!`);
}

